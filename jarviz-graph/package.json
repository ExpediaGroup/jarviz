{
  "name": "@vrbo/jarviz-graph",
  "version": "0.1.7",
  "description": "Utility for graphing JAR coupling data provided via jarviz",
  "main": "lib/index.js",
  "bin": "./bin/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:example": "npm run build && NODE_ENV=production node bin/index --input=lib/mock --output=build",
    "clean": "rimraf build",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development webpack-dev-server",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpediaGroup/jarviz.git",
    "directory": "jarviz-graph"
  },
  "keywords": [
    "jarviz",
    "graphing"
  ],
  "author": {
    "name": "Expedia Group",
    "email": "oss@expediagroup.com"
  },
  "contributors": [
    "neenhouse",
    "lasanthak"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "html-webpack-inline-source-plugin": "0.0.10"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-destructuring": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "alphabetize-object-keys": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "commander": "^3.0.0",
    "copy-dir": "^1.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "eslint": "^6.2.2",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "js-beautify": "^1.10.0",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-force-graph": "^1.24.1",
    "rimraf": "^2.6.3",
    "style-loader": "^1.0.0",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  }
}
